

# tracker
include "RecoLocalTracker/Configuration/data/RecoLocalTracker_Cosmics.cff"
include "RecoTracker/Configuration/data/RecoTrackerP5.cff"
include "RecoVertex/BeamSpotProducer/data/BeamSpot.cff"


sequence trackerGR = {offlineBeamSpot,  trackerlocalreco, tracksP5}
sequence trackerGR_woPixels = {offlineBeamSpot,  striptrackerlocalreco, tracksP5}


#
### calorimeters
#

include "RecoLocalCalo/Configuration/data/RecoLocalCalo_Cosmics.cff"
include "RecoEcal/Configuration/data/RecoEcalCosmics.cff"

 sequence caloGR = { calolocalreco, ecalClusters }


#
### muons
include "RecoLocalMuon/Configuration/data/RecoLocalMuonCosmics.cff"
include "RecoMuon/Configuration/data/RecoMuonCosmics.cff"
sequence muonsLocalRecoGR = {muonlocalreco & muonlocalrecoNoDrift}
sequence muonsGR = {muonsLocalRecoGR, muonRecoGR}

#
### jets
#i
include "RecoJets/Configuration/data/RecoCaloTowersGR.cff"
include "RecoJets/Configuration/data/RecoJetsGR.cff"

sequence jetsGR = {recoCaloTowersGR, recoJetsGR}

include "RecoMET/Configuration/data/RecoMET.cff"
sequence metrecoGR= {metreco}

    sequence reconstructionGR = {trackerGR, caloGR, muonsGR,  jetsGR, metrecoGR}
    sequence reconstructionGR_woTracker = { caloGR, muonsGR,  jetsGR, metrecoGR}
    sequence reconstructionGR_woPixels = {trackerGR_woPixels, caloGR, muonsGR,  jetsGR, metrecoGR}

